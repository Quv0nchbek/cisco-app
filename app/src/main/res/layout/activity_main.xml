<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgte"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/circle"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src=""
        app:srcCompat="@drawable/circle"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="706dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/square"
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@drawable/square"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="706dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/triangle"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:rotation="-180"
        app:srcCompat="@drawable/triangle"
        tools:layout_editor_absoluteX="314dp"
        tools:layout_editor_absoluteY="706dp"
        tools:ignore="MissingConstraints" />

    <TextClock
        android:id="@+id/textClock"
        android:layout_width="101dp"
        android:layout_height="29dp"
        android:text="13:00"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/wifi"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:rotation="-360"
        app:srcCompat="@drawable/wifi"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/full_battery"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:rotation="-360"
        app:srcCompat="@drawable/full_battery"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="370dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/high_signal"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:rotation="-360"
        app:srcCompat="@drawable/high_signal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="339dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/cisco"
        android:layout_width="414dp"
        android:layout_height="202dp"
        android:rotation="-360"
        app:srcCompat="@drawable/cisco"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="78dp" />

    <ImageView
        android:id="@+id/module1"
        android:layout_width="463dp"
        android:layout_height="185dp"
        app:srcCompat="@drawable/module1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-26dp"
        tools:layout_editor_absoluteY="289dp" />

    <ImageView
        android:id="@+id/module2"
        android:layout_width="463dp"
        android:layout_height="185dp"
        app:srcCompat="@drawable/module2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-26dp"
        tools:layout_editor_absoluteY="484dp" />
</androidx.constraintlayout.widget.ConstraintLayout>